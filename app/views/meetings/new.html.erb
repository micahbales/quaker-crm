<h1>Add a New Meeting</h1>

<%= form_for @meeting do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <%= f.label :website %>
  <%= f.text_field :website %>
  <%= f.label :affiliation %>
  <%= f.text_field :affiliation %>
  <%= f.submit "Submit", class: "button" %>

<% end %>
