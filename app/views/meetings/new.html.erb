<h1>Add a New Meeting</h1>

<%= form_for @meeting do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br />
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br />
  <%= f.label :website %>
  <%= f.text_field :website %>
  <br />
  <%= f.label :affiliation %>
  <%= f.text_field :affiliation %>
  <br />
  <%= f.submit "Submit" %>

<% end %>
